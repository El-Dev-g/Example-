rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: INSECURE RULES
    // These rules allow any authenticated user full read and write access
    // to the entire database. This is for debugging purposes ONLY.
    // You MUST secure your database before deploying to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
